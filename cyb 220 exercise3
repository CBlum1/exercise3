    print("False")

# key words "and" and "or"
x = ( 7 > 3 and 10 < 13 )
print(x)
y = ( 2 > 3 or 17 < 13 )
print(y)

#testing if an item is in a list
Test_list = [ 3, 6, 9, 12, 15]
print("Checking to see if 18 exists in the code")
exist_count = Test_list.count(18)
if exist_count < 15:
    print('That is false 18 does not exist in this list')
else:
    print('That is true 18 does exist in this list')


List_2 = [1, 2, 3, 4, 5]
print("Is it True that this list has 0 in it?")
exist_count2 = List_2.count(0)
if exist_count2 < 1:
    print("No it is false 0 is not in this list")
else:
    print("yes it is true 0 is in this list")

