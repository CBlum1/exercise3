#Equality and Inequality in Strings
s1= 'Marble'
s2= "marble"
s3= "marble"
if s2 == s3:
    print('Its true the strings are equal')
else:
    print('Its False the strings are not equal')
s1= 'Yellow'
s2= 'Yellow'
s3= 'yellow'
if s1 == s3:
    print('Its True the strings are equal')
else:
    print('Its False, the strings are not equal')

#Tests using the lower method
School = 'AnDErSon UnIvEsItY'
print("Is YoUr sCHOol =='AnDErSon UnIvEsItY', YeS ThAt Is TruE.".lower())
print(School == 'AnDErSon UnIvEsItY' )
print("Is YoUr sCHOol =='LaNDer UniVERsItY', nO ThAt Is FaLSe.".lower())
print(School == 'LaNDer UniVERsItY')

#Numerical test
num = 8
if num >= 3:
    print("True")
else:
    print("False")

